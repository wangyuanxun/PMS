                            <li><a href="">权限管理1 </a></li>
                        </ul>
                    </li>
                </ul>
            </div>
        </div>
        <div class="vp-right">
            <div class="nav">
                系统管理 / 我的团队
            </div>
            <div class="content">
                @RenderBody()
            </div>
            <div class="loading">
